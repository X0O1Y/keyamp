{
  "title": "Media controls using command key",
  "rules": [
    {
      "description": "Decrease brightness ⌘ + f1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "display_brightness_decrement"
            }
          ]
        }
      ]
    },
    {
